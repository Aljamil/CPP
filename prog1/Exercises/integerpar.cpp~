# include <iostream>
using namespace std;

int main()
{
 int z;
 cin >> z;
 if (z > 0) {
 cout << "is positive" << endl;
 }
 else if (z < 0) {
 cout << "is negative" << endl;
 }
 else if (z == 0) {
 cout << "is zero" << endl;
 }
return 1;
}

